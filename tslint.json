{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-parameter-properties": false,
    "triple-equals": false,
    "no-debugger": false,
    "no-trailing-whitespace": false,
    "member-ordering": false,
    "no-this-assignment": [
      true,
      {
        "allowed-names": [
          "^self$",
          "^that$"
        ],
        "allow-destructuring": true
      }
    ],
    "only-arrow-functions": [
      false,
      "allow-declarations",
      "allow-named-functions"
    ],
    "no-empty": [
      false,
      "allow-empty-catch"
    ],
    "no-unnecessary-type-assertion": false,
    "return-undefined": false,
    "no-for-in-array": false,
    "comment-format": [
      true,
      "check-space"
    ],
    "no-var-keyword": true,
    "no-console": false,
    "no-any": true,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}